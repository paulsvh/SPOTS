<h2>Sign Up to Find Your New Favorite Spot!</h2>

<%= render partial: 'layouts/errors' %>

<%= form_for @user do |f| %>
    Username: <%= f.text_field :username %><br><br>
    Email: <%= f.email_field :email %><br><br>
    Password: <%= f.password_field :password %><br><br>
    <%= f.submit "Sign Up" %>
<% end %>
